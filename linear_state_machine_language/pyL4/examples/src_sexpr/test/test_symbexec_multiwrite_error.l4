(TimeUnit m)

(ContractParams
	(N : Nat)
)

(StateVars
	(n : Nat = N)
	(v : Nat = 0)
)

(FormalContract "double write error"
	(StartSituation Ongoing)

	(Situation Ongoing
		(Next
			(DoIt immediately)
		)
	)

	(Action DoIt
		(StateTransform
			(if (n < 1000) (
				(v' = n)

			) else ())
			(if (n > 900) (
				(v' = (2 * n))
			) else ())
		)
		(TransitionsTo Fulfilled)
	)
)