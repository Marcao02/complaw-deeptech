
(Roles I)

(GlobalVars
	(n : Nat = 3)
)

(TimeUnit d)

(FormalContract "test contract for must later"
	(StartSection Ongoing)

	(Section Ongoing
		(Next
			(DoIt immediately)
		)
	)

	(Action DoIt
		(StateTransform
			(local x : Nat = (n * n))
			(local y : Nat = (x * x))
		)
		(FollowingSection
			(Next
				((FinishIt y) immediately)
			)
		)
	)

	(Action (FinishIt (z : Nat))
		(StateTransform
			(n' = z)
		)
		(TransitionsTo Fulfilled)
	)
)