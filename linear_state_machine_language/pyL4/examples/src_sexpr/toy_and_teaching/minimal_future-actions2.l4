[Roles I]

[TimeUnit d]

[FormalContract "test contract 2 for must later"
	[StartSituation Ongoing]

	[Situation Ongoing
		[Next
			[I may [Throw ?n] no_time_constraint]
			[I may EnterFulfilled no_time_constraint]
		]

		(possibly-from-earlier I must-later Catch)
	]

	[Action [Throw [n : Nat]]		[TransitionsTo Ongoing]
		[Future
			[I must-later [Catch n] no_time_constraint]
		]
	]

	[Action [Catch [n : Nat]] [TransitionsTo Ongoing]]
	[Action EnterFulfilled [TransitionsTo Fulfilled]]
]